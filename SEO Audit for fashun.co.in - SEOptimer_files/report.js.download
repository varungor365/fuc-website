            }
        });
    } else {
        $('#options-btn').on('touch click', function (e) {
            const self = $(this);
            setTimeout(function () {
                if ($(self).attr('data-kt-menu-attach') !== 'parent') {
                    $(self).addClass('btn-active');
                    $(self).removeClass('btn-light-primary');
                }
            }, 1);
            setTimeout(function () {
