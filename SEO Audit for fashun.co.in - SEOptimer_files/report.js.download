                </tr>\
            </thead>\
            <tbody>';

    const headings = [];
    let heading_max = 0;
    let length = 0;
    for (let i = 2; i < 7; i++) {
        length = 0;
        if (check.data['h' + i] !== undefined) {
            length = check.data['h' + i]['length'];
        }
        headings.push([
            'H' + i,
            length
        ]);

