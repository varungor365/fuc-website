function csrfFunction(parameter) {
    // sha1 (maybe)
    if (typeof SERDFDFVHA === 'undefined'){
        console.warn('csrf function not exixts');
        return '';
    }

    return SERDFDFVHA(parameter);
}

$(function () {
    history.scrollRestoration = "manual";
