/*product grid*/
.owl-carousel .owl-wrapper{ display: none;}
.product-grid-item {
  margin: 0 15px 30px;
  float: left;
  display: inline-block;
}
.product-wrapper{
  /*float: none;
  display: block;
  overflow: hidden;*/
  display: inline-block;
  float: left;
  max-width: 100%;

  .product-head{
    position: relative;
    float: none;

    .product-image{
      a{
        position: relative;
        display: block;
        overflow: hidden;

        &:after{
          content:"";
          @include ab(0,0,0,0);
          width: 100%;
          height: 100%;
          background: $black;
          @include opacity(0);
          z-index: 2;
        }

        img{
          display: block;
        }

      }
    }

    .wrapper-countdown{
      @include ab(n,0,43%,0);
      width: 100%;
      height: 40px;
      z-index: 1;

      > div{
        border: 0;
        background: #f6f6f6;
        line-height: 40px;
        color: $body-link-color;
        font-size: 12px;
        text-align: center;
      }
    }

    .product-button{
      text-align: center;
      @include ab(43%,0,n,n);
      width: 100%;
      @include opacity(0);     
      -webkit-backface-visibility: hidden;
      -moz-backface-visibility: hidden;
      z-index: 5;
      -webkit-transition: opacity 0.35s, -webkit-transform 0.35s;
      transition: opacity 0.35s, transform 0.35s;
      -webkit-transform: translate3d(0, 20px, 0);
      transform: translate3d(0, 20px, 0);

      .product-button-inner{
        display: inline-block;      

        .add-cart-button{
          margin-bottom: 10px;
          min-width: 160px;
          a{
            width: 100%;
          }

        }

        .quick-shop-button{
         margin-right: 5px;
         margin-bottom: 10px;
         text-align: center;
         padding-top: 11px;
        }
        .contact-form{
          display: inline;
        }
        .add-to-wishlist{
          width: 41px;
          height: 41px;
          display: inline-block; 
          padding: 11px;
          box-sizing: border-box;
          -webkit-box-sizing: border-box;
          -moz-box-sizing: border-box;
          vertical-align: top;
          margin-bottom: 10px;
          background: #fff;
          border: 0;

          .icon-wl{
            width: 20px;
            height: 16px;
            background-position: -46px -98px;
          }

          &:hover{
            background: #e18a07;
            .icon-wl{
              background-position: -38px -144px;
            }
          }
        }
      }
    }

    &:hover{
      .product-image{
        a{
          img{
           
          }
          &:after{
            @include opacity(0.6);
          }
        }
      }
      .product-button{
        @include opacity(1);
        top: 37%;
        -moz-transition: all 0.3s ease-in-out 0s;
        -webkit-transition: all 0.3s ease-in-out 0s;
        -ms-transition: all 0.3s ease-in-out 0s;
        -o-transition: all 0.3s ease-in-out 0s;
        transition: all 0.3s ease-in-out 0s;
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
      }
    }
  }

  .product-review{      
      margin-top: 5px;      

      .spr-badge-starrating {
        color: $body-link-hover-color;
        .spr-icon{       
          margin: 0 1px;          
        }
      }
      .spr-badge-caption{
        display: none;
      }
    }
  .product-content{
    display: block;
    margin-top: 20px; 

    .pc-inner{
      display: block;
      width: 100%;
      float: left;      
    }

    .product-title{
      text-transform: uppercase;
      font-size: $body-font-size; 
      margin-bottom: 10px;
      color: $body-link-color;
      font-weight: 600;

      &:hover a{
        text-decoration: none;
      }
    }

    .product-descrip{
      display: none;
    }

    .product-price{
      margin: 0px 0 10px;
      font-size: {{ settings.size_h5 }};
      color: $body-price-color;      

      .price-compare{
        margin-bottom: 10px;
        display: inline;
      }

      .price-sale{
        margin-right: 10px;
      }

    }
  }
}

.price-sale{
  color: $body-price-sale-color;
  font-weight: 600;
  margin-right: 10px;
}

.price-compare{
  //color: $body-title-color;
  font-weight: 400;
  text-decoration: line-through;
  margin-right: 10px;
 
}

.product-label{
  z-index: 4;
  @include ab(15px,n,n,10px);
  font-size: $body-font-size;
  text-transform: uppercase;
  color: $body-banner-color;
  
  > span{
    border-radius: 100%;
    height: 70px;
    width: 70px;    
    display: inline-block;
    text-align: center;
    padding: 15px;


    &.label-sale{    
      color: $body-label-sale-color;
      background-color: $body-label-sale-background;
    }

    &.label-deal{    
      color: $body-label-deal-color;
      background-color: $body-label-deal-background;
    }
    > span{
      border-top: 1px solid $body-label-sale-color;
      border-bottom: 1px solid $body-label-sale-color;
      display: inline-block;
      > span{
        font-size: 15px;
        font-weight: 600;
        display: block;
      }
    }
    
  }
 
}
.quick-shop-button{
  
  display: inline-block;
  width: 41px;
  height: 41px;
  cursor: pointer;
  background: #fff;

  .icon-quick-view{
    background-position: 0 -101px;
    width: 24px;
    height: 12px;
  }
  &:hover{
    background: #e18a07;
     .icon-quick-view{
        background-position: 0 -144px;
      }
  }
