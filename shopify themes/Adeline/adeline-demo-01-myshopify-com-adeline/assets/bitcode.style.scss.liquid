              content: "";
              @include ab(2px,0,n,n);
              width: 12px;
              height: 12px;
              background-color: $white;
              border: 1px solid $body-border-color;
            }

            &:after{
              content: "";
              @include ab(5px,2px,n,n);
              width: 8px;
              height: 6px;
              @include opacity(0);
              background: url({{"check-icon.png" | asset_url}}) no-repeat;

