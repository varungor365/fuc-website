/*button slider*/
.owl-theme {
  .owl-controls .owl-buttons > div{  
    @include ab(40%,n,n,n);  
    display: inline-block;
    padding: 0;
    margin: 0;
    border-radius: 0;
    background-color:transparent;
    @include opacity(0);
    @include CSStransition(all,500ms,ease-in-out,0s);  
    width: 40px;
    height: 40px;
    text-align: center;
    background-color: $body-background-color;
    padding: 9px; 

    &.owl-prev{
      span{
        background-position: -201px 2px;
        width: 15px;
        height: 20px;
      }
      left: 0px;

      &:hover{
        span{
          background-position: -201px -31px;
        }
      }
    }

    &.owl-next{
      span{
        background-position: -237px 2px;
        width: 15px;
        height: 20px;
      }
      
      right: 0px;
      &:hover{
        span{
          background-position: -237px -31px;
        }
      }
    }

    &:hover{
      background-color: #fff;
    }
  }

  &:hover{
    .owl-controls .owl-buttons > div{
      @include opacity(1);
    }
  }
}

