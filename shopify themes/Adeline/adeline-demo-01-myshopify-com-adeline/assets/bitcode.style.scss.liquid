/* header ----------------*/
.header-content{
  position: relative;
  z-index: 1030; /*fixed z-index higher than zoomContainer*/
  background-color: $header-background;
  &.header-fixed{
    position: fixed;
    z-index: 1030; /*fixed z-index higher than zoomContainer*/
    left: 0;
    right: 0;
    top: 0; 
  }
  .header-top{    
    background-color: $header-top-background;
    color: $header-link;
    text-transform: uppercase;
    //border-bottom: 1px solid $header-line;
    @include CSStransition(all,0.3s,ease-out,0s);
    font-size: 12px;
    line-height: 1.35;
    letter-spacing: 0.2em;
    text-align: right;
    width: 100%;
    float: left;

    .customer-links{      
      padding-right: 0px;
      border-left: 1px solid $header-line;

      a{
        color: $header-link;
        padding: 10px 12px 13px 10px;
        border-right: 1px solid $header-line;
        display: inline-block;

        &:hover{
          text-decoration: none;
          color: $header-hover-color;
          .icon-wl{
            background-position: -29px -42px;
          }
        }

        .icon{
          margin-right: 7px;
        }
        .icon-wl{
          width: 15px;
          height: 16px;
          background-position: -29px 2px;
        }

        &#customer_login_link, &#customer_account_link{
          .icon-login{
            width: 15px;
            height: 16px;
          }
          &:hover{
            .icon-login{
              background-position: 0 -44px;
            }
          }
        }

      }
    }

    .multi-currency{        
      position: relative;
      color: $header-link;
      padding-bottom: 20px;
      margin-bottom: -20px;
      padding-left: 20px !important;
      float: left;      

      a{
        color: $header-hover-color;

        &:hover{
          text-decoration: none;
        }

        i{
          font-size: 12px;
          margin-right: 7px;
        }
      }

      span, i{
        color: $header-link;
      }

      ul.currencies{
        border: 1px solid #eee;
        width: 100%;
        box-sizing: border-box;
        min-width: 90px;        
        left: 1px;
        right: 0;

        .active{

          a{
            background-color: #eee;
            &:hover{
              cursor: default;
            }
          }
        }

        li{
          margin-bottom: 0;
          padding: 0;

          a{
            padding: 2px 5px 2px 19px;
            font-weight: 300;
            font-size: $body-font-size;
          }

          &:hover{
            a{
              background-color: #eee;
            }
          }
        }
      }

      &:hover{
        ul.currencies{
          display: block;
        }
      }
    }

    .ht-left {
      float: left;
      display: inline-block;
      font-size: 12px;
      line-height: 1.35;
     p{ 
      margin-top: 0;
     }  
     li{
      border-left: 1px solid $header-line;
      padding: 13px 12px 14px;
      &:first-child{
        border-left: 0;
      }
      &.hotline a{ 
        color: $header-link;
      }
     }    
    }

    .ht-right {
      //@include CSStransition(all,0.5s,ease-out,0s);
      display: inline-block;
      
    }
  }

  .header-bottom{

    background-color: $header-background;
    padding-top: 38px;
    width: 100%;
    float: left;
    > .container{
      position: relative;
    }

    .logo-site{
      text-align: center;
      padding-top: 5px;
      padding-bottom: 15px;
     // @include CSStransition(all,0.5s,ease-out,0s);

        .light-logo{
          display: none;
        }
      
      a{
        display: inline-block;
        color: {{ settings.logo_color }};
        font-size: {{ settings.logo_size }};
        line-height: 78px;

        img {
          //@include CSStransition(all,0.5s,ease-out,0s);
          /*height: 88px;*/
          width: auto;
          display: block;
          float: left;

          &.sticky-logo{
            display: none;
          }
        }

        &:hover{
          color: {{ settings.logo_hover_color }};
          text-decoration: none;
        }
      }
    }

    .navigation {
      //@include CSStransition(all,0.5s,ease-out,0s);
    }
    
    .sc-toggle{
      margin-top: 12px;
      ul{
        text-align: right;
      }

    }

    &.dark{
      .logo-site{
        .light-logo{
          display: none;
        }
      }
      .navigation .navbar .navbar-collapse .main-nav>li>a{
        color: #292929;
      }
      .navigation .navbar .navbar-collapse .main-nav>li:hover a, .navigation .navbar .navbar-collapse .main-nav>li.active a {
        color: #e18a07;
      }
    }
    &.light{
      .logo-site{        
        .light-logo{
          display: block;
        }
      
        .dark-logo{
          display: none;
        }
      }
      .navigation .navbar .navbar-collapse .main-nav>li>a{
        color: #ffffff;
      }
      .navigation .navbar .navbar-collapse .main-nav>li:hover a, .navigation .navbar .navbar-collapse .main-nav>li.active a {
        color: #e18a07;
      }
      .search-toggle{
        background: #ffffff;

        .icon-search{
          background-position: -85px -91px;
        }
        &:hover{
          .icon-search{
            background-position: -80px -43px;
          }
        }

      }
    }
  }

  .cart-target{
    padding: 3px 12px;
    > a{          
      color: $header-link;
      position: relative;        
      display: inline-block;
      @include CSStransition(all,0.5s,ease-out,0s);

      &:hover{
        text-decoration: none;
        color: $header-hover-color;

        .icon-cart{          
          background-position: -55px -44px;
        }
      }

      .icon-cart{
        width: 15px;
        height: 17px;
        background-position: -55px 0px;
        
      }

      .number{
        color: $header-link;
        font-size: 11px;
        background-color: $header-hover-color;
        width: 20px;
        height: 20px;        
        display: inline-block;
        text-align: center;
        line-height: 20px;
        letter-spacing: 0;
        
      }

    }
  }

  .search-toggle{
    @include CSStransition(all,0.5s,ease-out,0s);
     cursor: pointer;
     background: $header-top-background;
     padding: 8px;
     width: 41px;
     height: 41px;

    .icon-search{
      width: 25px;
      height: 25px;
      background-position: -80px 0 ;
      /*background-color: red;*/

    }

    &:hover{
      .icon-search{
        background-position: -80px -44px;
      }
    }

  }

  /*stick menu*/
  &.header-fixed {
    background-color: transparent;

    .header-top {
      z-index: 10;
      background: transparent;
      padding: 0;
      border: 0;
      height: 0;    
      position: absolute;
      left: 0;
      right: 0;

      .ht-right {
        .customer-links a{
          > span:last-child{
            display: none;
          }
          .icon{
            margin-right: 0;
          }
        }
        
      }
    }

    .header-bottom {
      background: transparent;
      box-shadow: 0 0px 8px #ccc !important;
      padding-top: 0;

      .logo-site {
        display: none;
        padding-top: 5px;
        padding-bottom: 5px;
        text-align: center;

        a {
          line-height: 60px;
          > span{
            display: none;
          }

          img {
            /*height: 44px;*/

            &.default-logo{
              display: none;
            }

            &.sticky-logo{
              display: inline-block;
            }
          }

        }
      }

      &:before{
        content: "";
        @include ab(0,0,0,0);
        width: 100%;
        height: 100%;
        z-index: -1;
        background: $sticky-background-color !important;
        {% if settings.sticky_menu_bg_opacity %}
        @include opacity(0.9);
        {% endif %}

      }

      .sc-toggle{
        display: none;
      }
    }

    .navigation {
      margin-top: 5px;
      padding: 0 0px 0 0;

      .navbar-collapse .main-nav {
       min-height: 65px;

        > li {
          padding-bottom: 40px;
          margin-bottom: -40px;

          >a{
            color: $sticky-nav-text-color !important;
            line-height: 22px;
          }

          &:hover > a, &.active > a{
            color: $nav-link-hover-color !important;

            /*span:after{
              background-color: $sticky-nav-text-color;
            }*/
          }
        }
      }

      .navbar {
        min-height: 65px;
      }
    }
  }

}

