    // Wait for window load
    $(window).load(function() {		
      var loader = $( '.se-pre-con' );
      if ( loader.length ) {
        $( window ).on( 'beforeunload', function() {
          loader.fadeIn( 500, function() {
            loader.children().fadeIn( 100 )
