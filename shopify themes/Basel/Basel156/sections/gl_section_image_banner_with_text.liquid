{%- if section.settings.section_margin_top != blank or section.settings.section_margin_bottom != blank or section.settings.section_border != blank -%}
{%- assign s_margin_top = section.settings.section_margin_top | remove: ' ' | join: ' ' -%}
{%- assign s_margin_bottom = section.settings.section_margin_bottom | remove: ' ' | join: ' ' -%}
{%- assign s_border = section.settings.section_border | remove: ' ' | split: '-' | join: ' ' -%}
{%- endif -%}
{%- capture style_block_section -%}
   {%- if section.settings.section_margin_top != blank or section.settings.section_margin_bottom != blank or section.settings.section_padding_top != blank or section.settings.section_padding_right != blank or section.settings.section_padding_left != blank or section.settings.section_padding_bottom != blank or section.settings.section_bg_color != blank or section.settings.section_bg_image != blank or section.settings.section_border != blank -%}
   <style type="text/css">.gl_custom_{{section.id}}{ {%- if section.settings.section_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if section.settings.section_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if section.settings.section_padding_top != blank -%}padding-top: {{section.settings.section_padding_top}} !important;{%- endif-%}{%- if section.settings.section_padding_right != blank -%}padding-right: {{section.settings.section_padding_right}} !important;{%- endif-%}{%- if section.settings.section_padding_bottom != blank -%}padding-bottom: {{section.settings.section_padding_bottom}} !important;{%- endif-%}{%- if section.settings.section_padding_left != blank -%}padding-left: {{section.settings.section_padding_left}} !important; {%- endif -%}{%- if section.settings.section_bg_color != blank -%}background-color: {{ section.settings.section_bg_color }} !important;{%- endif -%}{%- if section.settings.section_bg_image != blank -%}{%- assign image = section.settings.section_bg_image -%}{%- assign image_size = image.width | append:'x' -%} background-image: url({%-include 'gl_image_format',src: image , size: image_size -%}) !important; background-position: center !important; background-repeat: no-repeat !important; background-size: cover !important;{%- endif -%}{%- if section.settings.section_border != blank and section.settings.section_border_color != blank -%}border-width: {{ s_border }} !important; border-color: {{section.settings.section_border_color}} !important; border-style: {{section.settings.section_border_style}} !important{%- endif -%} }</style>
   {%- endif -%}
{%- endcapture -%}
{%- capture data_section -%}
  {%- if section.settings.row_stretch == 'stretch_row' -%}
   {%- assign class_has_fill = 'gl_row-has-fill' -%}
   data-gl='has_section_gl_js' data-gl-full-width='true' data-gl-full-width-init='true'
  {%- elsif section.settings.row_stretch == 'stretch_row_content' or section.settings.row_stretch == 'stretch_row_content_no_spaces' -%}
   {%- assign class_has_fill = 'gl_row-has-fill' -%}
   data-gl='has_section_gl_js' data-gl-full-width='true' data-gl-full-width-init='true' data-gl-stretch-content='true'
  {%- else -%}
  {%- assign class_has_fill = '' -%}
  {%- endif -%}
{%- endcapture -%}
{{ style_block_section }}
<div {{ data_section }}{%- if section.settings.use_video and section.settings.video_url != blank %} data-gl-video-bg="{{ section.settings.video_url }}"{%- endif %} class="gl_custom_{{section.id}} {% if section.settings.row_stretch == 'stretch_row_content_no_spaces' %}gl_row-no-padding{%- endif %}{%- if section.settings.use_parallax -%} basel-parallax{%- endif %} gl_row spb_row gl_row-fluid {{class_has_fill}} {%- if section.settings.use_video and section.settings.video_url != blank %} opacity-video-bg gl_video-bg-container gl_has_youtube{%- endif %}{%- if section.settings.full_height %} gl_row-o-full-height gl_row-o-columns-{{section.settings.columns_position}}{%- endif %}{%- if section.settings.equal_height %} gl_row-o-equal-height{%- endif %}{%- unless section.settings.content_position == 'default' %} gl_row-o-content-{{section.settings.content_position}}{%- endunless %}{%- if section.settings.equal_height or section.settings.content_position != 'default' %} gl_row-flex{%- endif -%}">
   {%- if section.settings.use_gradient and section.settings.color_gradient_one != blank and section.settings.color_gradient_two != blank -%}
   <div class="basel-row-gradient"></div>
   {%- endif -%}
   {%- for block in section.blocks -%}
      {%- case block.type -%}
         {%- when 'image' -%}
            {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank -%}
               {%- assign s_margin_top = block.settings.block_margin_top | remove: ' ' | join: ' ' -%}
               {%- assign s_margin_bottom = block.settings.block_margin_bottom | remove: ' ' | join: ' ' -%}
               {%- assign s_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
            {%- endif -%}
            {%- capture style_block_section -%}
               {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank or block.settings.block_bg_image != blank -%}
               style="{%- if block.settings.block_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if block.settings.block_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{s_padding}} !important;{%- endif -%}{%- if block.settings.block_bg_image != blank -%} background-image: url({{ block.settings.block_bg_image | img_url: '2048x' | split:'?' | first }}) !important; background-position: center !important;background-repeat: no-repeat !important;background-size: cover !important{%- endif -%}"
               {%- endif -%}
            {%- endcapture -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width | plus:0 }}">
               <div class="gl_column-inner" {{ style_block_section }}>
                  <div class="spb_wrapper">
                     <div class="spb_single_image gl_mb_0 spb_content_element gl_align_{{ block.settings.image_alignment_one }}">
                        <figure class="spb_wrapper gl_figure">
                           <div class="gl_single_image-wrapper gl_box_border_grey {{ block.settings.image_style_one }}">
                              {%- if block.settings.block_image != blank -%}
                              {%- assign image = block.settings.block_image -%}
                              {%- assign image_size = image.width | append:'x' -%}
                              {%- assign image_width = image.width -%}
                                 <img width="{{ image.width }}" height="{{ image.height }}" class="info-icon image-1 attachment-full" alt="{{ image.alt }}"
                                 src="{%-include 'gl_image_format',src: image , size: image_size -%}" 
                                 srcset="{%-include 'gl_image_format',src: image , size: image_size %} {{image_width}}w 
                                 {%- if image_width > 300 -%},{%-include 'gl_image_format',src: image , size: '300x' %} 300w{%- endif -%} 
                                 {%- if image_width > 280 -%},{%-include 'gl_image_format',src: image , size: '270x' %} 270w{%- endif -%} 
                                 {%- if image_width > 280 -%},{%-include 'gl_image_format',src: image , size: '280x' %} 280w{%- endif -%} 
                                 {%- if image_width > 273 -%},{%-include 'gl_image_format',src: image , size: '273x' %} 273w{%- endif -%} 
                                 {%- if image_width > 546 -%},{%-include 'gl_image_format',src: image , size: '546x' %} 546w{%- endif -%}" sizes="(max-width: {{ image_width }}px) 100vw, {{ image_width }}px">
                              {%- else -%}
                                 <img width="600" height="442" src="https://cdn.shopify.com/s/files/1/1933/6253/files/portfolio-placeholder_600x600_crop_center.jpg?12790048884060573015">                            
                              {%- endif -%}
                           </div>
                        </figure>
                     </div>
                  </div>
               </div>
            </div>
         {%- when 'text' -%}
            {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank -%}
               {%- assign s_margin_top = block.settings.block_margin_top | remove: ' ' | join: ' ' -%}
               {%- assign s_margin_bottom = block.settings.block_margin_bottom | remove: ' ' | join: ' ' -%}
               {%- assign s_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
            {%- endif -%}
            {%- capture style_block_section -%}
               {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank or block.settings.block_bg_image != blank -%}
               style="{%- if block.settings.block_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if block.settings.block_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{s_padding}} !important;{%- endif -%}{%- if block.settings.block_bg_image != blank -%} background-image: url({{ block.settings.block_bg_image | img_url: '2048x' | split:'?' | first }}) !important; background-position: center !important;background-repeat: no-repeat !important;background-size: cover !important{%- endif -%}"
               {%- endif -%}
            {%- endcapture -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width | plus:0 }}">
               <div class="gl_column-inner color-scheme-{{ block.settings.text_color }} text-{{ block.settings.text_alignment }}" {{ style_block_section }}>
                  <div class="spb_wrapper">
                     {%- if block.settings.custom_heading != blank -%}<h5 style="font-size: 18px;line-height: 1.1;margin-bottom: 10px" class="gl_custom_heading">{{ block.settings.custom_heading }}</h5>{%- endif -%}
                     {%- if block.settings.custom_heading_two != blank -%}<h2 style="font-size: 56px;line-height: 1.1;margin-bottom: 5px" class="gl_custom_heading"><strong>{{ block.settings.custom_heading_two }}</strong><br></h2>{%- endif -%}
                     {%- if block.settings.custom_heading_three != blank -%}<h2 style="font-size: 22px;color: rgba(255,255,255,0.9);line-height: 1.1;margin-bottom: 15px;font-weight:300;font-style:normal" class="gl_custom_heading">{{ block.settings.custom_heading_three }}</h2>{%- endif -%}
                     {%- if block.settings.text_block != blank -%}<div class="spb_text_column spb_content_element color-scheme-light" style="margin-bottom: 25px">
                        <div class="spb_wrapper">
                           <p>{{ block.settings.text_block }}</p>
                        </div>
                     </div>{%- endif -%}
                     {%- if block.settings.button_label != blank and block.settings.button_link != blank -%}
                     <div class="basel-button-wrapper text-{{ block.settings.text_alignment }}">
                        <a href="{{ block.settings.button_link }}" class="btn btn-color-{{ block.settings.button_color }} btn-style-{{ block.settings.button_style }} btn-size-{{ block.settings.button_size }}">{{ block.settings.button_label }}</a>
                     </div>
                     {%- endif -%}
                  </div>
               </div>
            </div>
         {%- when 'text_2' -%}
            {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank -%}
               {%- assign s_margin_top = block.settings.block_margin_top | remove: ' ' | join: ' ' -%}
               {%- assign s_margin_bottom = block.settings.block_margin_bottom | remove: ' ' | join: ' ' -%}
               {%- assign s_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
            {%- endif -%}
            {%- capture style_block_section -%}
               {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank or block.settings.block_bg_image != blank -%}
               style="{%- if block.settings.block_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if block.settings.block_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{s_padding}} !important;{%- endif -%}{%- if block.settings.block_bg_image != blank -%} background-image: url({{ block.settings.block_bg_image | img_url: '2048x' | split:'?' | first }}) !important; background-position: center !important;background-repeat: no-repeat !important;background-size: cover !important{%- endif -%}"
               {%- endif -%}
            {%- endcapture -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width | plus:0 }}">
               <div class="gl_column-inner gl_pb_0 color-scheme-{{ block.settings.text_color }} text-{{ block.settings.text_alignment }}" {{ style_block_section }}>
                  <div class="spb_wrapper">
                     {%- if block.settings.custom_heading != blank -%}<h2 style="font-size: 38px;font-family:Satisfy;font-weight:400;font-style:normal, margin-bottom: 5px" class="gl_custom_heading">{{ block.settings.custom_heading }}</h2>{%- endif -%}
                     <div class="title-wrapper basel-title-color-{{ block.settings.title_color }} basel-title-style-{{ block.settings.title_style }} basel-title-size-{{ block.settings.title_size }} text-{{ block.settings.title_align }}" {{ style_block_section }}>
                        {%- if block.settings.sub_title != blank -%}
                        <span class="title-subtitle font-{{ block.settings.subtitle_font }}">{{ block.settings.sub_title }}</span>
                        {%- endif -%}
                        {%- if block.settings.title != blank -%}
                        <div class="liner-continer"> 
                           <span class="left-line"></span> 
                           <span class="title">{{ block.settings.title }}<span class="title-separator"><span></span></span></span> 
                           <span class="right-line"></span> 
                        </div>
                        {%- endif -%}
                        {%- if block.settings.text_after_title != blank -%}
                        <span class="title-after_title">{{ block.settings.text_after_title }}</span>
                        {%- endif -%}
                     </div>
                     {%- if block.settings.button_label != blank and block.settings.button_link != blank -%}
                     <div class="basel-button-wrapper text-{{ block.settings.text_alignment }}">
                        <a href="{{ block.settings.button_link }}" class="btn btn-color-{{ block.settings.button_color }} btn-style-{{ block.settings.button_style }} btn-size-{{ block.settings.button_size }}">{{ block.settings.button_label }}</a>
                     </div>
                     {%- endif -%}
                  </div>
               </div>
            </div>
         {%- when 'text_3' -%}
            {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank -%}
               {%- assign s_margin_top = block.settings.block_margin_top | remove: ' ' | join: ' ' -%}
               {%- assign s_margin_bottom = block.settings.block_margin_bottom | remove: ' ' | join: ' ' -%}
               {%- assign s_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
            {%- endif -%}
            {%- capture style_block_section -%}
               {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank or block.settings.block_bg_image != blank -%}
               style="{%- if block.settings.block_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if block.settings.block_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{s_padding}} !important;{%- endif -%}{%- if block.settings.block_bg_image != blank -%} background-image: url({{ block.settings.block_bg_image | img_url: '2048x' | split:'?' | first }}) !important; background-position: center !important;background-repeat: no-repeat !important;background-size: cover !important{%- endif -%}"
               {%- endif -%}
            {%- endcapture -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width }}">
               <div class="gl_column-inner color-scheme-{{ block.settings.text_color }} text-{{ block.settings.text_alignment }}" {{ style_block_section }}>
                  <div class="spb_wrapper">
                     <div class="spb_text_column spb_content_element gl_mb_0">
                        <div class="spb_wrapper">
                           {{ block.settings.heading }}
                        </div>
                     </div>
                     <div class="spb_text_column spb_content_element gl_pd_lr_10">
                        <div class="spb_wrapper">
                           {{ block.settings.text }}
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         {%- when 'text_fashion' -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width }} color-scheme-{{ block.settings.text_color }}">
               <div class="gl_column-inner ">
                  <div class="spb_wrapper">
                     {%- assign b_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
                     {%- assign b_border = block.settings.block_border | remove: ' ' | split: '-' | join: ' ' -%}
                     {%- capture style_block-%}
                           style="{%- if block.settings.block_margin_top != blank %}margin-top: {{block.settings.block_margin_top}};{%- endif -%}{%- if block.settings.block_margin_bottom != blank %}margin-bottom: {{block.settings.block_margin_bottom}};{%- endif -%}{%- if block.settings.block_margin_right != blank %}margin-right: {{block.settings.block_margin_right}};{%- endif -%}{%- if block.settings.block_margin_left != blank %}margin-left: {{block.settings.block_margin_left}};{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{b_padding}} !important;{%- endif -%}{%- if block.settings.block_border != blank and block.settings.block_border_color != blank -%}border-width: {{ b_border }} !important; border-color: {{block.settings.block_border_color}} !important; border-style: {{block.settings.block_border_style}} !important{%- endif -%}"
                     {%- endcapture -%}
                     <div class="spb_text_column spb_content_element gl_custom_{{block.id}} text-{{ block.settings.text_alignment }}" {{ style_block }}>
                        <div class="spb_wrapper">
                           {{ block.settings.banner_content }}
                           {%- if block.settings.banner_link != blank and block.settings.banner_btn_title != blank -%}
                             {%- assign button_style_color = block.settings.button_style_color -%}
                             <p><a class="btn btn-color-{{ block.settings.button_color }} btn-size-{{block.settings.button_size}} {{ button_style_color }}" href="{{ block.settings.banner_link }}">{{ block.settings.banner_btn_title }}</a></p>
                           {%- endif -%}
                        </div>
                     </div>
                  </div>
               </div>
            </div>  
         {%- when 'custom_html' -%}
            {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank -%}
               {%- assign s_margin_top = block.settings.block_margin_top | remove: ' ' | join: ' ' -%}
               {%- assign s_margin_bottom = block.settings.block_margin_bottom | remove: ' ' | join: ' ' -%}
               {%- assign s_padding = block.settings.block_padding | remove: ' ' | split: '-' | join: ' ' -%}
            {%- endif -%}
            {%- capture style_block_section -%}
               {%- if block.settings.block_margin_top != blank or block.settings.block_margin_bottom != blank or block.settings.block_padding != blank or block.settings.block_bg_image != blank -%}
               style="{%- if block.settings.block_margin_top != blank -%} margin-top: {{s_margin_top}} !important;{%- endif -%}{%- if block.settings.block_margin_bottom != blank -%} margin-bottom: {{s_margin_bottom}} !important;{%- endif -%}{%- if block.settings.block_padding != blank -%}padding: {{s_padding}} !important;{%- endif -%}{%- if block.settings.block_bg_image != blank -%} background-image: url({{ block.settings.block_bg_image | img_url: '2048x' | split:'?' | first }}) !important; background-position: center !important;background-repeat: no-repeat !important;background-size: cover !important{%- endif -%}"
               {%- endif -%}
            {%- endcapture -%}
            <div class="spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width }} color-scheme-light">
               <div class="gl_column-inner color-scheme-{{ block.settings.text_color }} text-{{ block.settings.text_alignment }}">
                  <div class="spb_wrapper">
                     <div class="spb_text_column spb_content_element" {{ style_block_section }}>
                        <div class="spb_wrapper">{{ block.settings.custom_html }}</div>
                     </div>
                  </div>
               </div>
            </div>
         {%- when 'text_bicycle' -%}
            <div class="text-{{ block.settings.text_alignment }} spb_column gl_column_container gl_col-sm-{{ block.settings.banner_width }}">
               <div class="gl_column-inner gl_mb_20">
                  <div class="spb_wrapper">
                     <div class="spb_text_column spb_content_element gl_pt_0 color-scheme-{{ block.settings.text_color }}">
                        <div class="spb_wrapper">
                           <h1 style="font-size: 52px; font-weight: 400;">{{ block.settings.text_block }}</h1>
                        </div>
                     </div>
                     {%- if block.settings.button_label != blank and block.settings.button_link != blank -%}
                     <div class="basel-button-wrapper text-{{ block.settings.text_alignment }}">
                        <a href="{{ block.settings.button_link }}" class="btn btn-color-{{ block.settings.button_color }} btn-style-{{ block.settings.button_style }} btn-size-{{ block.settings.button_size }}">{{ block.settings.button_label }}</a>
                     </div>
                     {%- endif -%}
                  </div>
               </div>
            </div>
      {%- endcase -%}
   {%- endfor -%}
</div>
{%- unless section.settings.row_stretch == 'default' -%}
  <div class="gl_row-full-width gl_clearfix"></div>
  {%- endunless -%}
{%- schema -%}
  {
    "name": "Image with text overlay",
    "class": "index-section has_section_gl_js",
    "settings": [
      {
      "type": "header",
      "content": "== General"
      },
      {
        "type": "select",
        "id": "row_stretch",
        "options": [
         {
            "value": "default",
            "label": "Default"
          },
         {
            "value": "stretch_row",
            "label": "Stretch row"
          },
          {
            "value": "stretch_row_content",
            "label": "Stretch row and content"
          },
          {
            "value": "stretch_row_content_no_spaces",
            "label": "Stretch row and content (no paddings)"
          }
        ],
        "label": "___ Row stretch",
        "info": "Select stretching options for row and content (Note: stretched may not work properly if parent container has 'overflow: hidden' CSS property).",
        "default": "default"
      },
      {
        "type": "checkbox",
        "id": "full_height",
        "label": "Full height row?",
        "info": "if checked row will be set to full height.",
        "default": false
      },
      {
        "type": "select",
        "id": "columns_position",
        "options": [
         {
            "value": "middle",
            "label": "Middle"
          },
         {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          },
          {
            "value": "stretch",
            "label": "Stretch"
          }
        ],
        "label": "___ Columns position (only for full height)",
        "info": "Select columns position within row.",
        "default": "middle"
      },
      {
        "type": "checkbox",
        "id": "equal_height",
        "label": "Equal height?",
        "info": "If checked columns will be set to equal height.",
        "default": false
      },
      {
        "type": "select",
        "id": "content_position",
        "options": [
          {
            "value": "default",
            "label": "Default"
          },
         {
            "value": "middle",
            "label": "Middle"
          },
         {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ],
        "label": "___ Content position",
        "info": "Select content position within columns.",
        "default": "default"
      },
      {
        "type": "checkbox",
        "id": "use_video",
        "label": "Use video background?",
        "info": "If checked, video will be used as row background.",
        "default": false
      },
      {
        "id": "video_url",
        "type": "video_url",
        "label": "YouTube link",
        "accept": ["youtube"],
        "default": "https://www.youtube.com/watch?v=flTcrOns02I"
      },
      {
        "type": "header",
        "content": "== Design Options"
      },
      {
        "type": "image_picker",
        "id": "section_bg_image",
        "label": "Background Image"
      },
      {
        "type": "color",
        "id": "section_bg_color",
        "label": "Background color"
      },
      {
