        {%- when 'page' -%}
          {{ pages[block.settings.home_page_content].content }}
        {%- when 'text' -%}
          {{ block.settings.home_page_richtext }}
       {%- when 'custom_html' -%}
          {{ block.settings.code }}
      {%- endcase -%}
      </div>
    </div></div>
  </div>
{%- endfor -%}
