  {%- else -%}
  {%- assign class_has_fill = '' -%}
  {%- endif -%}
{%- endcapture -%}
{{style_block_section}}
<div {{ data_section }}{%- if section.settings.use_video and section.settings.video_url != blank %} data-gl-video-bg="{{ section.settings.video_url }}"{%- endif %} class="gl_custom_{{section.id}} {% if section.settings.row_stretch == 'stretch_row_content_no_spaces' %} gl_row-no-padding{%- endif %}{%- if section.settings.use_parallax %} basel-parallax{%- endif %} gl_row spb_row gl_row-fluid {%- if section.settings.use_video and section.settings.video_url != blank %} opacity-video-bg gl_video-bg-container gl_has_youtube{%- endif %}{%- if section.settings.full_height %} gl_row-o-full-height gl_row-o-columns-{{section.settings.columns_position}}{%- endif %}{%- if section.settings.equal_height %} gl_row-o-equal-height{%- endif -%}">
{%- for block in section.blocks -%}
  {%- if block.settings.padding != blank or block.settings.margin != blank -%}
  {%- assign margin = block.settings.margin | remove: ' ' | split: '-' | join: ' ' -%}
  {%- assign padding = block.settings.padding | remove: ' ' | split: '-' | join: ' ' -%}
  {%- endif -%}
  {%- capture style_block -%}
     {%- if block.settings.body_bg_image != blank -%}
        {%- if block.settings.body_bg != blank -%}background-color: {{ block.settings.body_bg }} !important;{%- endif -%}
        background-image: url({{ block.settings.body_bg_image | img_url: 'master' | split:'?' | first  }}) !important;
        {%- if block.settings.body_bg_position != 'default' -%}
        {%- case block.settings.body_bg_position -%}
        {%- when 'left_top' -%}
           {%- assign position = "left top" -%}
        {%- when 'left_center' -%}
           {%- assign  position = "left center" -%}
        {%- when 'left_bottom' -%}
           {%- assign position = "left bottom" -%}
        {%- when 'center_top' -%}
           {%- assign position = "center top" -%}
        {%- when 'center_center' -%}
           {%- assign position = "center center" -%}
        {%- when 'center_bottom' -%}
           {%- assign position = "center bottom" -%}
        {%- endcase -%}
        background-position: {{ position }} !important;
        {%- endif -%}
        {%- if block.settings.body_bg_repeat != 'default' -%}background-repeat: {{ block.settings.body_bg_repeat }} !important;{%- endif -%}
        {%- if block.settings.body_bg_size != 'default' -%}background-size: {{ block.settings.body_bg_size }} !important;{%- endif -%}
     {%- else -%}
        background-color: {{ block.settings.body_bg }};
     {%- endif -%}
     {%- if block.settings.margin != blank -%} margin: {{margin}} !important;{%- endif -%}{%- if block.settings.padding != blank -%}padding: {{padding}} !important{%- endif -%}
  {%- endcapture -%}
  {%- capture class_block -%}
     {%- if block.settings.offset_width != 'none' -%} gl_col-md-offset-{{ block.settings.offset_width | plus: 0 }}{%- endif -%}{%- if block.settings.offset_width_Small != 'none' -%} gl_col-sm-offset-{{ block.settings.offset_width_Small | plus: 0 }}{%- endif -%}{%- if block.settings.offset_width_Extra != 'none' -%} gl_col-xs-offset-{{ block.settings.offset_width_Extra | plus: 0 }}{% endif %}{% if block.settings.hide_medium == 'on' %} hidden-md{% endif %}{% if block.settings.hide_Small == 'on' %} hidden-sm{% endif %}{% if block.settings.hide_Extra == 'on' %} hidden-xs{%- endif -%}
  {%- endcapture -%}
  <div class="spb__column gl_column_container color-scheme-{{ block.settings.color_scheme }} text-{{ block.settings.text_align }} gl_col-xs-{{ block.settings.block_width_Extra | plus: 0 }} gl_col-sm-{{ block.settings.block_width_Small | plus: 0 }} gl_col-md-{{ block.settings.block_width_medium | plus: 0 }} gl_col-lg-{{ block.settings.block_width | plus: 0 }} {{ class_block }}" {{ block.shopify_attributes }}>
     <div class="gl_column-inner "><div class="spb_wrapper">
       <div class="tea_custom_{{ block.id }}" style="{{ style_block }}">
