               {%-capture variantSoldout-%}
               {%-for option in product.options_with_values-%}
                   {%-for value in option.values-%}
                     {% if option.selected_value == value %} bg_{{value | handleize}}{% endif-%}
                   {%-endfor-%}
               {%-endfor-%}
               {%-endcapture-%}
            {%-endunless-%}
            {%- if product.metafields.external -%}
