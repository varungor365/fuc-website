nav { 
  width: 100%; 
  background: {{ settings.navigation }}; 
  margin-bottom: 0px;

  z-index: 2;
}

ul#main-nav { 
  list-style: none; 
  text-align: center;
  margin-bottom: 0;
  position: relative;
}

ul#main-nav li { 
  display: inline-block; 
}


ul#main-nav li:hover { background: {{ settings.nav_hover_color }};  }

ul#main-nav li a { 
  font-family: {{ settings.navigation-font}}; 
  font-weight: {{ settings.nav-weight }}; 
  font-size: {{ settings.nav_size }}; 
  color: {{ settings.nav_color }}; 
  display: block; 
  padding: 0 20px; 
  text-transform: {{ settings.navigation-transform}};
}

ul#main-nav li a:hover {
  color: {{ settings.nav_hover_link_color }}; 
  text-decoration: none;
}

ul#main-nav li.dropdown > a:after {
  font-family: 'FontAwesome'; 
  content: "\f107";
  margin-left: 10px;
}

ul#main-nav li ul.submenu {
  display: none;
  position: absolute;
  z-index: 5;
  background: {{ settings.dropdown-background }};  
  border-bottom: 1px solid {{ settings.nav-border-color }};
  border-left: 1px solid {{ settings.nav-border-color }};
  border-right: 1px solid {{ settings.nav-border-color }};
}

ul#main-nav li:hover ul.submenu{
  display: block;
}

ul#main-nav li ul.submenu li{
  line-height: 34px! important;
  margin: 0; 
  padding: 0;
  white-space: nowrap;
  background: transparent! important;
  min-width: 220px;
  display: block;
  position: relative;
}

ul#main-nav li ul.submenu li:hover {
  background: {{ settings.dropdown-hover-background }}! important; 
}


ul#main-nav li ul.submenu li a{
  text-align: left;
  font-weight: normal;
  text-transform: none! important;
  color: {{ settings.dropdown-link-color }};
  font-size: {{ settings.font-size }}! important;
  font-family:{{ settings.font }}! important;
}


ul#main-nav li ul.submenu li a:hover {
  color: {{ settings.dropdown-link-hover-color }};
}


.submenu li.nest {
}


ul#main-nav li ul.submenu li.nest:after {
  position: absolute;
  top: 0px;
  right: 10px;
  font-family: 'FontAwesome'; 
  content: "\f105";
}

.submenu li.nest:hover > ul.nested {
  display: block; 
}

.nested {
  display: none;
  position: absolute;
  left: 100%;
  top: 0;  
  background: {{ settings.dropdown-background }};  
  border: 1px solid {{ settings.nav-border-color }};  
}


/* Megamenu */

ul#main-nav li.dropdown:hover > ul.megamenu {
  display: block;
}


ul.megamenu {
  display: none;
  position: absolute;
  left: 0;
  z-index: 2;
  line-height: initial;
}



ul.megamenu li > ul {
  width: 100%;
}

ul.megamenu {
  background: {{ settings.dropdown-background }};  
  border: 1px solid {{ settings.nav-border-color }};
  padding-bottom: 20px;
}

ul.megamenu h4 {
  padding: 20px 0 5px;
  font-family: {{ settings.navigation-font}}! important; 
  color: {{ settings.dropdown-link-color }};
  font-weight: bold;
  border-bottom: 1px solid {{ settings.dotted_color }};
  margin-bottom: 10px;
  text-align: left;
  font-size: {{ settings.nav_size }}; 
  letter-spacing: initial;
}

ul.megamenu li:hover {
  background: transparent! important;
}

ul.megamenu li > ul.mega-stack {
  background: {{ settings.dropdown-background }};
}

ul.megamenu li > ul.mega-stack li {
  line-height: 20px! important;
  text-align: left! important;
  display: block! important;
  clear: left; 
}

ul.megamenu li > ul.mega-stack li a { 
  padding: 0! important;  
  font-weight: normal! important;
  color: {{ settings.dropdown-link-color }}! important;
  font-size: {{ settings.font-size }}! important;
  font-family:{{ settings.font }}! important;
  background: transparent! important;
  text-transform: none! important;
}


ul.megamenu li > ul.mega-stack li a:hover {
  color: {{ settings.dropdown-link-hover-color }}! important;
}


.megamenu img {
  margin-top: 20px;
}



.collection div.zoomWindow, .index div.zoomWindow, .search div.zoomWindow {
  display: none! important;
}
.collection div.zoomLens, .index div.zoomLens, .search div.zoomLens {
  opacity: 0! important;
}





