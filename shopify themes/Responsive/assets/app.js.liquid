            {% if settings.show_multiple_currencies %}
              var currentCurrency = $('#currencies').val();
              $(document).on('change', '#currencies', function(e) {
                  currentCurrency = $(this).val();
              });
              Currency.convertAll($addToCartForm.data('shop-currency'), currentCurrency);
            {% endif %}
          }, 500);

          window.setTimeout(function(){
            $addToCartBtn.removeAttr('disabled').removeClass('disabled');
            $addToCartBtn.find('span').addClass("fadeOut").text($addToCartBtn.data('label')).removeClass('fadeIn').removeClass("fadeOut").addClass('zoomIn');
          }, 1000);

          $.getJSON("/cart.js", function(cart) {
            refreshCart(cart);
          });
        },
