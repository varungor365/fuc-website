  }
}

#open-me a { color: {{ settings.password_page_text_color }}; }

.close-me { z-index: 8000; }
.close-me a { color: #000; }

.modalbox {
  position: absolute;
  height: 100%;
  top: -1em;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 0;
  z-index:5000;
}

.overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  max-height: 100%;
  top: 0;
  left: 0;
  background: #fff;
  background: rgba(255,255,255,0.95);
}

.overlay-close {
  width: 150px;
  height: 50px;
  position: absolute;
  right: 10px;
  top: 10px;
  border: 1px solid #000;
  font-size: 14px;
}
.overlay-data {
  opacity: 0;
  visibility: hidden;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
  visibility: 0s 0.5s;
  transition: opacity 0.5s, visibility 0s 0.5s;
}
.overlay-open {
  opacity: 1;
  visibility: visible;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
}

.inputbox {
  margin: 0px auto;
  max-width: 100%;
  display: table;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-right: -50%;
  -ms-transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  color: #000;
}

