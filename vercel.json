{
  "buildCommand": "cd fashun-store && npm run build",
  "installCommand": "cd fashun-store && npm install",
  "redirects": [
    {
      "source": "/shop",
      "destination": "/collections/all",
      "permanent": true
    },
    {
      "source": "/privacy",
      "destination": "/privacy-policy", 
      "permanent": true
    }
  ]
}